@model SeasonDetailVM
@{
    ViewData["Title"] = "Detail";
}

<div class="container my-3">
    <h1 class="text-center">Slider detail</h1>
    <div class="slider-detail">
        <h3 class="my-3">Name : @Model.Name</h3>
        <h3 class="my-3">Film : @Model.Film</h3>
        <h3 class="my-3">Create date : @Model.CreatedDate</h3>
    </div>
    <a asp-action="Index" class="btn btn-primary btn-rounded btn-fw">Back</a>

</div>